@article{zhangTwinscrewASDTug2023,
  title = {Twin-Screw {{ASD}} Tug Maneuvering Prediction Based on Integrated {{CFD}} and Empirical Methods},
  author = {Zhang, Song and Wu, Qing and Liu, Jialun and He, Yangying and Li, Shijie},
