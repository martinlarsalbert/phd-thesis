  langid = {english},
  keywords = {skip},
  file = {C\:\\Zotero\\storage\\CYQF5WMY\\Zhang et al. - 2019 - What Is the Predictability Limit of Midlatitude We.pdf;C\:\\Zotero\\storage\\3I54PCEA\\jas-d-18-0269.1.html}
}

